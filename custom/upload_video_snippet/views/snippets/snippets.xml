<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="video_snippet" inherit_id="website.snippets" name="video snippet">
        <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
            <t t-snippet="upload_video_snippet.s_video"
               t-thumbnail="/upload_video_snippet/static/src/img/snippets_thumbs/s_video.svg"/>
        </xpath>
    </template>

</odoo>